  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.7.0</version>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-surefire-plugin</artifactId>
        <version>2.20.1</version>
        <configuration>
	  <excludes>
            <exclude>**/DataProviderProxyTest.java</exclude>
	    <exclude>**/IFDataStoreProxyTest.java</exclude>
          </excludes>
        </configuration>
      </plugin>
      <plugin>
	<groupId>org.pitest</groupId>
	<artifactId>pitest-maven</artifactId>
	<version>1.4.7</version>
	<configuration>
	  <excludedTestClasses>
	    <param>eu.supersede.integration.api.analysis.proxies.test.DataProviderProxyTest</param>
	    <param>eu.supersede.integration.api.datastore.proxies.test.IFDataStoreProxyTest</param>
	    <param>eu.supersede.integration.api.adaptation.proxies.test.AdapterProxyTest</param>
	  </excludedTestClasses>	
	  <threads>16</threads>
	  <timeoutConstant>30000</timeoutConstant>:
	  <mutationEngine>descartes</mutationEngine>
	  <outputFormats>
	      <value>JSON</value>
	      <value>METHODS</value>
	      <value>ISSUES</value>
	      <value>HTML</value>
	  </outputFormats>
	</configuration>
	<dependencies>
	    <dependency>
	      <groupId>eu.stamp-project</groupId>
	      <artifactId>descartes</artifactId>
	      <version>1.2.5</version>
	    </dependency>
	</dependencies>
      </plugin>
    </plugins>
